{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "amigoscode-api",
      "image": "aboubacar/amigoscode-api:31.12.2023.21.07.45",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
     "environment": [
       {
         "name": "SPRING_PROFILES_ACTIVE",
         "value": "test"
       }

     ]
    },
    {
      "name": "sonarqube",
      "image": "aboubacar/sonarqube",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 9000,
          "containerPort": 9000
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "sonarqube_data",
          "containerPath": "/opt/sonarqube/data",
          "readOnly": false
        }
      ],
      "volumes": [
        {
          "name": "sonarqube_data"
        }
      ]
    }
  ]
}